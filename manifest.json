{
    "manifest_version": 2,
    "name": "Posture",
    "version": "0.1",
    "background": {
        "scripts": [
            "src/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/jquery-3.4.1.min.js",
                "src/posenet.js",
                "src/content.js",
                "lib/p5.min.js",
                "lib/p5.dom.min.js",
                "lib/ml5.min.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "img/piedPiperGreen.png"
    },
    "permissions" : [
        "storage",
        "activeTab"
    ]
}